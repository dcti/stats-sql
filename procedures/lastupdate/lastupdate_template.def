use stats
go

\set procedure=p_lastupdate_${1}

\echo "Creating procedure $procedure, referencing table statproc.${2}"

create procedure $procedure
as
	/* NOTE!!!!
	** You should not be calling this procedure directly, it should
	** be called from 'lastupdate'.
	*/

	select max(last) as lastupdate
	from statproc.${2}
	
	return
go

grant execute on $procedure to public
go
