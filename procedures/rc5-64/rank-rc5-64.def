use stats
go

drop procedure p_rank_rc5_64_e_o_rank
go

create procedure p_rank_rc5_64_e_o_rank
(	@id numeric(7,0) = 0		/* Participant ID to look up */
)
as
	/* NOTE!!!!
	** You should not be calling this procedure directly, it should
	** be called from 'rank'.
	*/

	declare @rank		int		/* Rank of participant */

	select @rank = rank
	from statproc.rc5_64_CACHE_em_RANK
	where id = @id
	
	return @rank
go

grant execute on p_rank_rc5_64_e_o_rank to public
go


